{
"minimum-stability": "dev",
    "name": "dshovchko/php_uapay",
    "description": "Library for UAPAY service integration",
    "keywords": ["uapay", "uapay.ua"],
    "homepage": "https://github.com/dshovchko/php_UAPAY",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry Shovchko",
            "email": "d.shovchko@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/dshovchko/php_UAPAY/issues",
        "source": "https://github.com/dshovchko/php_UAPAY"
    },
    "require": {
        "php": ">=5.5.27",
        "dshovchko/debulog": "^1.1",
        "guzzlehttp/guzzle": "~6.0",
        "firebase/php-jwt": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6",
        "php-coveralls/php-coveralls": "^2.2.0"
    },
    "autoload": {
        "psr-4": {
            "UAPAY\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UAPAYTest\\": "test/"
        }
    }
}
